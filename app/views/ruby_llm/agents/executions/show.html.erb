    <!-- Second Row: Tracing and Caching -->
    <div class="grid grid-cols-1 md:grid-cols-2 gap-6 mt-6">
      <!-- Tracing Information -->
      <% if @execution.trace_id.present? || @execution.request_id.present? %>
